{
  "name": "yaml-docgen",
  "version": "0.0.1",
  "description": "YAML comments to documentation markdown",
  "preferGlobal": true,
  "main": "./dist/bin/index.js",
  "bin": {
    "yaml-docgen": "./dist/bin/index.js"
  },
  "author": "Fles <the.final.frontend@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/Fles/yaml-docgen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fles/yaml-docgen.git"
  },
  "bugs": {
    "url": "https://github.com/Fles/yaml-docgen/issues"
  },
  "keywords": [
    ".yaml",
    ".yml",
    ".md",
    "yaml",
    "yml",
    "md",
    "markdown",
    "cicd",
    "parse",
    "documentation",
    "comments",
    "generator",
    "parser",
    "automatization"
